generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id String @id @default(uuid())
  name String
  email String @unique
  password String
}
 
model CAR {
  id String @id @default(uuid())
  carName String 
  Img String[]
  brand String
  price Int
  Fuel String
  Seat String 
  Mileage Int
  Availability Boolean
  model String
  Plate Int
  Year String
  type String
  Transmission String
  Color String 
}